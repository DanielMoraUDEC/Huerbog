@model IEnumerable<Huerbog.Models.UserList.UserListModel>
@{
    ViewData["Title"] = "indexAdmin";
    Layout = "_LayoutRegistrado";
}

<link rel="stylesheet" href="~/css/nav.css" />
<script >

    /* Set the width of the side navigation to 250px */
    function openNav() {
        document.getElementById("mySidenav").style.width = "155px";
    }

    /* Set the width of the side navigation to 0 */
    function closeNav() {
        document.getElementById("mySidenav").style.width = "0";
    }
</script>

<div id="mySidenav" class="sidenav">
    <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
    <a href="#">Inicio</a>
    <a href="#">Otro</a>
    <a href="#">Otro</a>
</div>

<!-- Use any element to open the sidenav -->
<span onclick="openNav()" class="text-primary h4">Abrir</span>



<div id="main">
    <form class="form-horizontal" role="form">
        <center>
            <br />
            <h1>Página de administración del sitio</h1>
        </center>
        <br /> <hr />

        <div class="container mb-2">
            <a asp-action="listUsers" class="h3 btn-outline-secondary text-dark ">Ver usuarios registrados</a>

        </div>
        <div class="container mb-2">
            <a asp-action="" class="h3 btn-outline-secondary text-dark ">Ver publicaciones reportadas</a>
        </div>


    </form>
</div>

